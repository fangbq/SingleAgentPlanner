(define (domain doors) 
   (:types pos)
   (:predicates (adj ?i ?j - pos) 
                (at ?i - pos)
                (opened ?i - pos)
   )
                
   (:action openDoor
    :parameters (?i - pos ?j - pos)
    :precondition(K (and (at ?i) (adj ?i ?j)))
    :effect (((True), (opened ?j))) 
   )

   (:action move
    :parameters (?i - pos ?j - pos )
    :precondition (K (and (adj ?i ?j) (at ?i) (opened ?j)))
    :effect (((True), (at ?j, not (at ?i)))) 
   )
)
